    "version": "1.0.0",
    "type": "library",
    "require": {
      "php": ">=5.3.0"
    },
    "license": "Apache-2.0",
    "authors": [
      {
        "name": "Giannis Ftaras",
        "email": "me@giannisftaras.dev"
      }
    ],
    "minimum-stability": "dev",
